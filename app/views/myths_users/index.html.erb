<p id="notice"><%= notice %></p>

<h1>Listing Myths Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Myth</th>
      <th>Access level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myths_users.each do |myths_user| %>
      <tr>
        <td><%= myths_user.user_id %></td>
        <td><%= myths_user.myth_id %></td>
        <td><%= myths_user.access_level %></td>
        <td><%= link_to 'Show', myths_user %></td>
        <td><%= link_to 'Edit', edit_myths_user_path(myths_user) %></td>
        <td><%= link_to 'Destroy', myths_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myths user', new_myths_user_path %>
