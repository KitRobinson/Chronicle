<p id="notice"><%= notice %></p>

<h1>Listing Associations</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th>Associable</th>
      <th>Associable type</th>
      <th>Strength</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associations.each do |association| %>
      <tr>
        <td><%= association.domain_id %></td>
        <td><%= association.associable_id %></td>
        <td><%= association.associable_type %></td>
        <td><%= association.strength %></td>
        <td><%= link_to 'Show', association %></td>
        <td><%= link_to 'Edit', edit_association_path(association) %></td>
        <td><%= link_to 'Destroy', association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Association', new_association_path %>
