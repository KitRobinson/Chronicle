
<%=render "mapping"%>

<div id="map-panel" class="container">
	<div class="row">
		<div class="col s8 side-bar">
			<img src="/assets/ireland.jpg" id="ireland-map" alt="ireland" usemap="#iremapping"/>
		</div>
		<div class="col s4 side-bar">
			<ul id="map-tablist" class="tabs">
		        <li id="geo-tab" class="tab col s3"><a href="#geo-inf" class="material-icons tooltipped" data-position="bottom" data-delay="50" data-tooltip="Geography"  style="font-size:36px;">language</a></li>
		        <li id="pol-tab" class="tab col s3"><a href="#pol-inf" class="material-icons tooltipped" data-position="bottom" data-delay="50" data-tooltip="Political"  style="font-size:36px;">supervisor_account</a></li>
		        <li id="rel-tab" class="tab col s3"><a href="#rel-inf" class="material-icons tooltipped" data-position="bottom" data-delay="50" data-tooltip="Religious"  style="font-size:36px;">vpn_key</a></li>
		        <li id="vis-tab" class="tab col s3"><a href="#vis-inf" class="material-icons tooltipped" data-position="bottom" data-delay="50" data-tooltip="Visibility"  style="font-size:36px;">visibility</a></li>
      		</ul>
			<div id="geo-inf" class="col s12"></div>
			<div id="pol-inf" class="col s12"></div>
			<div id="rel-inf" class="col s12"></div>
			<div id="vis-inf" class="col s12"></div>
		</div>
	</div>
</div>
